<ul class="organizations">
    <% for partner in @partners: %>
    <li itemscope itemtype="http://schema.org/Organization">
        <a href="<%= partner.url %>" itemprop="url" rel="external">
            <img src="<%= partner.logo %>" alt="<%= partner.name %>" itemprop="image">
        </a>
    </li>
    <% end %>
</ul>